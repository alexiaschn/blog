@book{druckerGraphesisVisualForms2014,
  title = {{Graphesis: Visual Forms of Knowledge Production}},
  shorttitle = {{Graphesis}},
  author = {Drucker, Johanna},
  year = 2014,
  month = jul,
  edition = {Illustr\'e \'edition},
  publisher = {Harvard University Press},
  address = {Cambridge, Massachusetts London},
  abstract = {Dans notre culture actuelle satur\'ee d'\'ecran, nous recueillons plus d'informations par des moyens visuels qu'\`a tout moment de l'histoire. Les ordinateurs et les smartphones qui nous inondent constamment d'images font plus que simplement transmettre des informations. Ils structurent notre relation \`a l'information \`a travers des formats graphiques. Apprendre \`a interpr\'eter comment les formes visuelles non seulement pr\'esentent mais produisent des connaissances, dit Johanna Drucker, est devenu une comp\'etence contemporaine essentielle. Graphesis fournit un langage critique descriptif pour l'analyse des connaissances graphiques. Dans une \'etude interdisciplinaire fusionnant les humanit\'es num\'eriques avec des \'etudes sur les m\'edias et l'histoire du design graphique, Drucker d\'ecrit les principes par lesquels les formats visuels organisent un contenu significatif. Parmi les plus importants de ces formats est l'interface utilisateur graphique (GUI), la caract\'eristique dominante des \'ecrans de presque tous les appareils \'electroniques grand public. Parce qu'une grande partie de notre vie personnelle et professionnelle est m\'edi\'ee par des interfaces visuelles, il est important de commencer \`a r\'efl\'echir de mani\`ere critique \`a la fa\c con dont elles fa\c connent la connaissance, notre comportement et m\^eme notre identit\'e. Les graphiques d'information portent des signes r\'ev\'elateurs des disciplines dans lesquelles ils sont originaires : statistiques, affaires et sciences empiriques. Drucker plaide pour \'etudier la visualit\'e d'un point de vue humaniste, explorant comment les langages graphiques peuvent servir des domaines o\`u les jugements qualitatifs ont la priorit\'e sur les d\'eclarations quantitatives de fait. Graphesis offre une nouvelle \'epist\'emologie de la fa\c con dont nous traitons l'information, embrassant le plein potentiel des formes visuelles et des formats de production de connaissances.},
  isbn = {978-0-674-72493-8},
  langid = {Anglais}
}

@article{druckerHumanitiesApproachesGraphical2011,
  title = {Humanities {{Approaches}} to {{Graphical Display}}},
  author = {Drucker, Johanna},
  year = 2011,
  journal = {Digital Humanities Quarterly},
  volume = {5},
  number = {1},
  issn = {1938-4122},
  urldate = {2025-03-14},
  abstract = {As digital humanists have adopted visualization tools in their work, they have borrowed methods developed for the graphical display of information in the natural and social sciences. These tools carry with them assumptions of knowledge as observer-independent and certain, rather than observer co-dependent and interpretative. This paper argues that we need a humanities approach to the graphical expression of interpretation. To begin, the concept of data as a given has to be rethought through a humanistic lens and characterized as capta, taken and constructed. Next, the forms for graphical expression of capta need to be more nuanced to show ambiguity and complexity. Finally, the use of a humanistic approach, rooted in a co-dependent relation between observer and experience, needs to be expressed according to graphics built from interpretative models. In summary: all data have to be understood as capta and the conventions created to express observer-independent models of knowledge need to be radically reworked to express humanistic interpretation.},
  copyright = {Creative Commons Attribution-NoDerivatives 4.0 International License},
  file = {/home/schn/snap/zotero-snap/common/Zotero/storage/QR3JSQAD/Drucker - 2011 - Humanities Approaches to Graphical Display.pdf}
}

