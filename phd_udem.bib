@article{sacyLireDifferentesEchelles2025,
  title = {Lire \`a Diff\'erentes \'Echelles. {{Une}} Analyse Multiscalaire de La Retrieval-Augmented Generation En Sciences Humaines et Sociales},
  author = {Sacy, Antoine Silvestre De and Faci, Adam and Maronet, L{\'e}a and Pouyllau, St{\'e}phane},
  year = 2025,
  journal = {Humanit\'es num\'eriques},
  volume = {12},
  issn = {2736-2337},
  doi = {10.4000/15icj},
  urldate = {2026-01-21},
  abstract = {Dans cet article, nous proposons une m\'ethodologie de recherche combinant les approches de retrieval-augmented generation, ou RAG, avec des traitements computationnels. Cette d\'emarche est appliqu\'ee \`a l'\'etude d'un corpus de revues en histoire de l'art et \`a l'analyse d'un concept fondamental et controvers\'e~: le style. \`A travers cet exemple, nous explorons les enjeux li\'es \`a l'int\'egration de ces m\'ethodes dans les sciences humaines, en mettant en lumi\`ere leurs apports potentiels tout en soulignant leurs limites. Loin de consid\'erer l'intelligence artificielle comme une solution universelle ou une fin en soi, notre m\'ethodologie invite au contraire \`a une appropriation critique de ces outils par les chercheurs. Elle insiste sur la n\'ecessit\'e de les adapter aux probl\'ematiques sp\'ecifiques des disciplines concern\'ees, \`a travers des cadres m\'ethodologiques diff\'erenci\'es appliqu\'es \`a des corpus familiers et ma\^itris\'es, tout en garantissant l'explicabilit\'e des r\'esultats.           ,              In this article, we propose a research methodology that combines retrieval-augmented generation (RAG) approaches with computational processing. This approach is applied to the study of a corpus of art history journals and the analysis of a fundamental and controversial concept: style. Through this example, we explore the challenges associated with integrating these methods into the humanities, highlighting their potential contributions while underlining their limitations. Far from considering artificial intelligence as a universal solution or an end in itself, our methodology instead calls for a critical appropriation of these tools by researchers. It emphasises the need to adapt them to the specific issues of the disciplines involved, through differentiated methodological frameworks applied to familiar and well-understood corpora, while ensuring the explicability of the results.},
  copyright = {https://creativecommons.org/licenses/by/4.0/},
  file = {/home/schn/snap/zotero-snap/common/Zotero/storage/4IZ9T53Z/Sacy et al. - 2025 - Lire √† diff√©rentes √©chelles. Une analyse multiscalaire de la retrieval-augmented generation en scien.pdf}
}

