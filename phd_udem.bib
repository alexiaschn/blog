@misc{jonesLargeLanguageModels2025,
  title = {Large {{Language Models Pass}} the {{Turing Test}}},
  author = {Jones, Cameron R. and Bergen, Benjamin K.},
  year = 2025,
  month = mar,
  number = {arXiv:2503.23674},
  eprint = {2503.23674},
