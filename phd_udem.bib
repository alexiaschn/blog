@article{cardonDansLespritPageRank2013,
  title = {Dans l'esprit Du {{PageRank}}: {{Une}} Enqu\^ete Sur l'algorithme de {{Google}}},
  shorttitle = {Dans l'esprit Du {{PageRank}}},
  author = {Cardon, Dominique},
  year = 2013,
