@misc{shahidLiteratureGroundedNoveltyAssessment2025,
  title = {Literature-{{Grounded Novelty Assessment}} of {{Scientific Ideas}}},
  author = {Shahid, Simra and Radensky, Marissa and Fok, Raymond and Siangliulue, Pao and Weld, Daniel S. and Hope, Tom},
  year = 2025,
