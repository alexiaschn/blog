@article{ramsaySpecialSectionReconceiving2003,
  title = {Special {{Section}}: {{Reconceiving Text Analysis}}: {{Toward}} an {{Algorithmic Criticism}}},
  shorttitle = {Special {{Section}}},
  author = {Ramsay, Stephen},
  year = 2003,
